    <script>
        function errorAlert(message) {
            Swal.fire({
                icon: 'error',
                text: message,
                timer: 60000,
                confirmButtonColor: "#FF9B44",
            })
        }

        function infoAlert(message) {
            Swal.fire({
                icon: 'info',
                text: message,
                timer: 30000,
                confirmButtonColor: "#FF9B44",
            })
        }

        function infoAlertWithStyles(message) {
            Swal.fire({
                title: 'Incomplete Payment',
                icon: 'info',
                text: message,
                confirmButtonColor: "#FF9B44",
            })
        }
        function newInfoAlert(message, url) {
            Swal.fire({
                icon: 'info',
                text: message,
                timer: 30000,
                confirmButtonColor: "#FF9B44",
            }).then((isConfirm) => {
                if (isConfirm) {
                    location.href = url;
                }
            })
        }
        function successAlert(message) {
            Swal.fire({
                icon: 'success',
                text: message,
                timer: 3000,
               // confirmButtonColor: "#FF9B44",
            })
        }

        function newErrorAlert(message, url) {
            Swal.fire({
                icon: 'error',
                text: message,
                timer: 1000,
                confirmButtonColor: "#FF9B44",
                width: "18em",
                height: "18em",
            }).then(function () {
                location.href = url;
            })
        }

        function newSuccessAlert(message, url) {
            Swal.fire({
                icon: 'success',
                text: message,
                confirmButtonColor: "#FF9B44",
            }).then((isConfirm) => {
                if (isConfirm) {
                    location.reload(url);
                }
            })

        }
        function successAlertWithRedirect(message, url) {
            Swal.fire({
                title: "Success",
                text: message,
                icon: "success",
                timer: 3000000,
                overlay: "background - color: rgba(43, 165, 137, 0.45)",
                confirmButtonColor: "black",
                height: "20em",
                width: "19em",
            }).then(function () {

                location.href = url;
            })
        }

        function successAlertWithFunction(message, action) {
            Swal.fire({
                title: "Success",
                text: message,
                icon: "success",
                timer: 3000000,
                overlay: "background - color: rgba(43, 165, 137, 0.45)",
                confirmButtonColor: "#FF9B44",
            }).then(function () {
                action;
            })
        }
    </script>
</head>

