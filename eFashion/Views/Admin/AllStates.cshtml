@model List<StateViewModel>

<h2>All States</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Delivery Fee</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var state in Model)
        {
            <tr>
                <td>@state.Name</td>
                <td>@state.DeliveryFee</td>
                
            </tr>
        }
    </tbody>
</table>
